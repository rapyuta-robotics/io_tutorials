{
	"apiVersion": "1.0.0",
	"name": "User Interface",
	"packageVersion": "v1.0.0",
	"description": "An interactive, web based version of ROS Turtlesim's user interface",
	"bindable": true,
	"plans": [{
		"name": "default",
		"metadata": {},
		"singleton": false,
		"components": [{
			"name": "user_interface",
			"description": "",
			"cloudInfra": {
				"replicas": 1,
				"endpoints": [{
					"name": "UserInterface",
					"exposeExternally": true,
					"port": 443,
					"targetPort": 8080,
					"proto": "HTTPS"
				}]
			},
			"ros": {
				"topics": [],
				"services": [],
				"actions": [],
				"isROS": false
			},
			"requiredRuntime": "cloud",
			"architecture": "amd64",
			"executables": [{
				"name": "uiexecutable",
				"simulationOptions": {
					"simulation": false
				},
				"cmd": [],
				"docker": "rrdockerhub/io_turtlesim_ui",
				"limits": {
					"cpu": 1,
					"memory": 4096
				}
			}],
			"parameters": []
		}],
		"includePackages": [],
		"dependentDeployments": [],
		"inboundROSInterfaces": {
		  "anyIncomingScopedOrTargetedRosConfig": true
		},
		"exposedParameters": []
	}]
}